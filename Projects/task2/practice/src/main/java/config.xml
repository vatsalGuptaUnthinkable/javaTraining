<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 <bean class="com.unthinkable.practice.Address" name = "ad1">
 <property name="city">
 <value>Kanpur</value>
 </property>
 <property name="street">
 <value>civil Lines</value>
 </property>
 
 </bean>
 
 
  <bean class="com.unthinkable.practice.Address" name = "ad2">
 <property name="city">
 <value>Delhi</value>
 </property>
 <property name="street">
 <value>Gurgaon</value>
 </property>
  </bean> 
  
 <bean  class="com.unthinkable.practice.Student" name = "student1">
 
 <property name="id">
 <value>1</value>
 </property>
 
 <property name="name">
 <value>Vatsal</value>
 </property>
 
 <property name="lt">
 <list>
 <value>111111</value>
 <value>222222</value>
 </list>
 </property>
 
 <property name="ad">
 <ref bean="ad1" />
 </property>
 
 </bean>
 
 
 <bean  class="com.unthinkable.practice.Student" name = "student2">
 <property name="id">
 <value>2</value>
 </property>
 
 <property name="name">
 <value>Deepak</value>
 </property>
 
 <property name="lt">
 <list>
 <value>111111</value>
 <value>222222</value>
 </list>
 </property>
 
 <property name="ad">
 <ref bean="ad2" />
 </property>
 
 </bean>
 
</beans>